{
  "name": "byoa",
  "version": "1.0.0",
  "description": "A native desktop assistant with AI-powered features built with React and Saucer",
  "homepage": "https://github.com/shmmsra/byoa",
  "repository": {
    "type": "git",
    "url": "https://github.com/shmmsra/byoa.git"
  },
  "bugs": {
    "url": "https://github.com/shmmsra/byoa/issues"
  },
  "keywords": [
    "desktop",
    "assistant",
    "ai",
    "macos",
    "native",
    "react",
    "saucer",
    "cpp"
  ],
  "author": "Shivam Mishra",
  "license": "MIT",
  "scripts": {
    "lint:js:check": "eslint src/web --ext .js,.jsx,.ts,.tsx",
    "lint:js:fix": "eslint src/web --ext .js,.jsx,.ts,.tsx --fix",
    "lint:css:check": "stylelint \"src/web/**/*.{css,scss}\"",
    "lint:css:fix": "stylelint \"src/web/**/*.{css,scss}\" --fix",
    "lint:web:check": "yarn lint:js:check && yarn lint:css:check",
    "lint:web:fix": "yarn lint:js:fix && yarn lint:css:fix",
    "start:web": "vite",
    "build:web:release": "vite build --emptyOutDir",
    "clean:web": "rimraf build/Resources",
    "lint:native:check": "find src/native -name '*.cpp' -o -name '*.hpp' -o -name '*.mm' | xargs clang-format --dry-run --Werror",
    "lint:native:fix": "find src/native -name '*.cpp' -o -name '*.hpp' -o -name '*.mm' | xargs clang-format -i",
    "configure:native": "cmake -B build -S .",
    "build:native:debug": "cmake --build build --config Debug",
    "build:native:release": "cmake --build build --config Release",
    "clean:native": "cmake --build build --target clean",
    "lint:all:check": "yarn lint:web:check && yarn lint:native:check",
    "lint:all:fix": "yarn lint:web:fix && yarn lint:native:fix",
    "build:all": "yarn build:web:release && yarn configure:native && yarn build:native:release",
    "clean:all": "yarn clean:web && yarn clean:native",
    "bundle": "echo \"This is a placeholder for the bundle script\""
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.2",
    "@types/diff": "^8.0.0",
    "antd": "^5.27.4",
    "classnames": "^2.5.1",
    "fast-levenshtein": "^3.0.0",
    "lucide-react": "^0.544.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.55.0",
    "react-router-dom": "^7.9.3"
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/plugin-proposal-class-properties": "^7.18.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.0",
    "@babel/preset-env": "^7.22.0",
    "@babel/preset-react": "^7.22.0",
    "@eslint/js": "^9.37.0",
    "@types/fast-levenshtein": "^0.0.4",
    "@types/node": "^20.10.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.8.0",
    "diff": "5.2.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.0",
    "sass": "^1.63.0",
    "sass-loader": "^13.3.0",
    "style-loader": "^3.3.0",
    "stylelint": "^16.25.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-prettier": "^5.0.3",
    "ts-loader": "^9.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.0",
    "vite": "6.3.5",
    "vite-plugin-svgr": "^4.5.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  }
}
